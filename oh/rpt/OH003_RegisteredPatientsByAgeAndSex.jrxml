	<queryString>
		<![CDATA[SELECT PAT_SEX AS SEX, "< 5" AS AGE, COUNT(PAT_ID) AS NUMBER
FROM PATIENT
WHERE DATE(PAT_TIMESTAMP) BETWEEN DATE($P{fromdate}) AND DATE($P{todate})
AND PAT_SEX = 'F'
AND PAT_AGE < 5
GROUP BY PAT_SEX

UNION

SELECT PAT_SEX AS SEX, "5 - 10" AS AGE, COUNT(PAT_ID) AS NUMBER
FROM PATIENT
WHERE DATE(PAT_TIMESTAMP) BETWEEN DATE($P{fromdate}) AND DATE($P{todate})
AND PAT_SEX = 'F'
AND PAT_AGE BETWEEN 5 AND 10
GROUP BY PAT_SEX

UNION

SELECT PAT_SEX AS SEX, "> 10" AS AGE, COUNT(PAT_ID) AS NUMBER
FROM PATIENT
WHERE DATE(PAT_TIMESTAMP) BETWEEN DATE($P{fromdate}) AND DATE($P{todate})
AND PAT_SEX = 'F'
AND PAT_AGE > 10
GROUP BY PAT_SEX

UNION

SELECT PAT_SEX AS SEX, "< 5" AS AGE, COUNT(PAT_ID) AS NUMBER
FROM PATIENT
WHERE DATE(PAT_TIMESTAMP) BETWEEN DATE($P{fromdate}) AND DATE($P{todate})
AND PAT_SEX = 'M'
AND PAT_AGE < 5
GROUP BY PAT_SEX

UNION

SELECT PAT_SEX AS SEX, "5 - 10" AS AGE, COUNT(PAT_ID) AS NUMBER
FROM PATIENT
WHERE DATE(PAT_TIMESTAMP) BETWEEN DATE($P{fromdate}) AND DATE($P{todate})
AND PAT_SEX = 'M'
AND PAT_AGE BETWEEN 5 AND 10
GROUP BY PAT_SEX

UNION

SELECT PAT_SEX AS SEX, "> 10" AS AGE, COUNT(PAT_ID) AS NUMBER
FROM PATIENT
WHERE DATE(PAT_TIMESTAMP) BETWEEN DATE($P{fromdate}) AND DATE($P{todate})
AND PAT_SEX = 'M'
AND PAT_AGE > 10
GROUP BY PAT_SEX]]>
