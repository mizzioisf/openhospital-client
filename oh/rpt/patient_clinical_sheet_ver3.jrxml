	<parameter name="Admission" class="java.lang.Boolean"/>
	<parameter name="Drugs" class="java.lang.Boolean"/>
	<parameter name="Examination" class="java.lang.Boolean"/>
	<parameter name="All" class="java.lang.Boolean"/>
